<p id="notice"><%= notice %></p>

<h1>Listing Links</h1>

<table>
  <thead>
    <tr>
      <th>Url</th>
      <th>Description</th>
      <th>Weighting</th>
      <th>Cluster</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @links.each do |link| %>
      <tr>
        <td><%= link.url %></td>
        <td><%= link.description %></td>
        <td><%= link.weighting %></td>
        <td><%= link.cluster_id %></td>
    <% end %>
  </tbody>
</table>

<br>
<%= link_to 'clusters', clusters_path %>
<%= link_to 'New Link', new_cluster_link_path %>
